    "moment": "2.20.1",
    "react-input-mask": "1.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-add-module-exports": "0.2.1",
